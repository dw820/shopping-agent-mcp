# Shopping Agent MCP Server Configuration
# Copy this file to .env and fill in your actual values

# Browser automation credentials
# Get these from your browser automation service provider
BROWSER_USE_API_KEY=your_actual_browser_use_api_key_here
BROWSER_USE_PROFILE_ID=your_actual_browser_use_profile_id_here

# Target website credentials
# These are for the shopping website you want to automate
TARGET_EMAIL=your_email@example.com
TARGET_PASSWORD=your_secure_password_here
PHONE_NUMBER=+1234567890

# Optional: Additional configuration
# DEBUG=true
# LOG_LEVEL=INFO